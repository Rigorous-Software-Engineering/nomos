import transpiler;
input x1;
var x2 := chBranchCond(x1, "java");
requires x2 != null;
output d1;
output d2;
{
d1 = transpiler.transpile(x1, "java", "py")
d2 = transpiler.transpile(x2, "java", "py")
}

ensures numLoops(d2, "py") == numLoops(x2, "java") ==> numLoops(d1, "py") == numLoops(x1, "java") ;